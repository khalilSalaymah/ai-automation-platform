# LLM Provider (groq or gemini)
# For RAG chat, use gemini provider
# GEMINI_API_KEY is used for both LLM and embeddings
LLM_PROVIDER=gemini
GEMINI_API_KEY=your_gemini_api_key_here

# Redis
REDIS_URL=redis://localhost:6379/0

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/ai_agents

# Pinecone (optional - can use PGVector instead)
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_INDEX=rag-chat-index
PINECONE_ENVIRONMENT=us-east-1

# Application
APP_NAME=rag-chat
APP_ENV=development
LOG_LEVEL=INFO

# Vector Store (pgvector or pinecone)
VECTOR_STORE=pgvector
